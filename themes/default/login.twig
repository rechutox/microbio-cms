{% extends 'layout.twig' %}

{% block content %}

	<h1>Login</h1><hr>

	<form action="/login" method="post">
		{{ csrf_field()|raw }}
		<input type="text" name="user" placeholder="Username" required><br>
		<input type="password" name="password" placeholder="Password" required><br>
		<button type="submit">Login</button>
	</form>

{% endblock %}